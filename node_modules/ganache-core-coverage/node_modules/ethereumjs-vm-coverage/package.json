    "test": "node ./tests/tester -a",
    "testAPI": "tape ./tests/api/*.js",
    "testBlockchain": "npm run build:dist && node --stack-size=1500 ./tests/tester -b --fork='Constantinople' --dist --excludeDir='GeneralStateTests'",
